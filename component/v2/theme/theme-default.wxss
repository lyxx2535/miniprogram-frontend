
/* 日历主要颜色相关样式 */

.default_color,
.default_weekend-color,
.default_handle-color,
.default_week-color {
    color: #0b705a;
}

.default_today {
    color: #fff;
    background-color: #e8e8e8;
}


.default_date-disable {
    color: #c7c7c7;
}

.default_choosed.default_date-disable {
    color: #e2e2e2;
    background-color: #c2afb6;
}

.default_prev-month-date,
.default_next-month-date {
    color: #e2e2e2;
}

.default_normal-date {
    color: #626664;
}


